{
    "name": "ghostwriter/empty-vs-strict-benchmark",
    "description": "Benchmarking empty vs strict comparisons in PHP",
    "license": "BSD-4-Clause",
    "authors": [
        {
            "name": "Nathanael Esayeas",
            "email": "nathanael.esayeas@protonmail.com",
            "homepage": "https://github.com/ghostwriter",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/ghostwriter/empty-vs-strict-benchmark",
    "support": {
        "issues": "https://github.com/ghostwriter/empty-vs-strict-benchmark/issues",
        "source": "https://github.com/ghostwriter/empty-vs-strict-benchmark",
        "docs": "https://github.com/ghostwriter/empty-vs-strict-benchmark",
        "rss": "https://github.com/ghostwriter/empty-vs-strict-benchmark/releases.atom",
        "security": "https://github.com/ghostwriter/empty-vs-strict-benchmark/security/advisories/new"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/ghostwriter"
        }
    ],
    "_comment": [
        "#BlackLivesMatter",
        "#FreePalestine",
        "#StandWithUkraine",
        "#StopGenocide"
    ],
    "require": {
        "php": "~8.4.0 || ~8.5.0",
        "ghostwriter/filesystem": "~0.1.1",
        "ghostwriter/shell": "~0.1.0"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "mockery/mockery": "~1.6.12",
        "phpbench/phpbench": "~1.4.1",
        "phpunit/phpunit": "~12.3.8",
        "symfony/var-dumper": "~7.3.3"
    },
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Tests\\Benchmark\\": "tests/Benchmark"
        }
    },
    "config": {
        "allow-plugins": {
            "ghostwriter/coding-standard": true
        },
        "platform": {
            "php": "8.4.999"
        }
    },
    "scripts": {
        "bin_composer": "vendor/ghostwriter/coding-standard/tools/phar/composer",
        "bin_infection": "vendor/ghostwriter/coding-standard/tools/phar/infection --ansi --min-covered-msi=0 --min-msi=0 --show-mutations --threads=max",
        "bin_normalize": "vendor/ghostwriter/coding-standard/tools/phar/composer-normalize",
        "bin_phive": "vendor/ghostwriter/coding-standard/tools/phar/phive",
        "bin_phpunit": "vendor/ghostwriter/coding-standard/tools/phar/phpunit --do-not-cache-result --colors=always",
        "bin_psalm": "vendor/ghostwriter/coding-standard/tools/phar/psalm --no-cache"
    }
}
